#! /bin/sh
export LD_LIBRARY_PATH=/lejos/libjna/usr/lib/arm-linux-gnueabi/:/lejos/libjna/usr/lib/jni/
classpath=/lejos/lib/ev3classes.jar:/lejos/libjna/usr/share/java/jna.jar:.
if [ $1 = "-cp" ]
then
        classpath="$2":$classpath
        echo classpath: $classpath
        shift 2
fi
/lejos/ejre*/bin/java -classpath $classpath $*
